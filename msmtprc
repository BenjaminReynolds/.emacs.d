defaults
host smtp.gmail.com
port 587
protocol smtp
auth on
tls on
tls_trust_file /usr/local/etc/openssl/cert.pem

account personal
from breyno127@gmail.com
user breyno127@gmail.com
passwordeval pass mail/personal | head -1

account work
from ben@zaarly.com
user ben@zaarly.com
passwordeval pass mail/work | head -1
